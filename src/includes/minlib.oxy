fn atoi(char* s) -> int {
    int result = 0;
    int i = 0;
    while (s[i] != 0) {
        result = result * 10 + (s[i] - '0');
        i += 1;
    }
    ret result;
}

fn strlen(char* s) -> int {
    int i = 0;
    while (s[i] != 0) {
        i += 1;
    }
    ret i;
}

fn print(char* a) -> void {
    puts(a);
}

fn print(int a) -> void {
    display_number(a);
}