extern atoi; // from system libc (for testing externs)

fn factorial(int n) -> int {
    if (n <= 1) {
        ret 1;
    }
    ret n * factorial(n-1);
}

fn wrapper(char* str) -> void {
    puts(str);
    int n = 5;
    while (1) {
        n += 1;
        if (n >= 5) {
            break;
        }
        continue;
    }
    puts("done loop");
}

fn main() -> int {
    int n = atoi("15");
    puts("String converted to integer");
    int result = factorial(5);
    puts("Factorial computed");
    result++;

    int x = 10 % 3;
    puts("Modulo computed");
    x = -x;
    puts("Negation computed");
    x = -x;

    ret n;
}